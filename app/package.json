{
  "name": "BalerDisplay",
  "version": "0.0.1",
  "description": "Baler Display",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bash npm-scripts/build.sh dev",
    "clean": "rm -rf build && rm -rf load_cell_addon/build && find . -name '*.js' -type f -not -path './node_modules/*' -delete && find . -name '*.js.map' -type f -not -path './node_modules/*' -delete",
    "tslint": "bash npm-scripts/tslint.sh",
    "tsc": "tsc || exit 0",
    "typings": "typings",
    "build_addon": "cd load_cell_addon && node-gyp rebuild --target=0.36.7  --dist-url=https://atom.io/download/atom-shell",
    "noop": "echo noop"
  },
  "author": "BJN",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "electron-prebuilt": "^0.36.7",
    "electron-rebuild": "^1.1.3",
    "tslint": "^3.5.0",
    "typescript": "^1.8.2",
    "typings": "^0.6.8"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-route": "~1.5.0",
    "angular-translate": "^2.10.0",
    "angular-translate-handler-log": "^2.10.0",
    "angular-ui-bootstrap": "^1.2.2",
    "angular-virtual-keyboard": "^0.4.3",
    "angular-xeditable-npm": "^0.1.9",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.1",
    "jqwidgets-framework": "^4.1.1",
    "nan": "^2.2.0",
    "nedb": "^1.8.0",
    "q": "^1.4.1",
    "webcam-directive": "~3.0.0"
  }
